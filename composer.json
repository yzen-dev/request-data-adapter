  "description": "Adapter for converting input data from Illuminate\\Http\\Request",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "keywords": [ "laravel", "request", "Illuminate","adapter", "trait" ],
  "authors": [
    {
      "name": "Andey Iatsenko",
      "email": "yzen.dev@gmail.com"
    }
